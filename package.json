    "mime-types": "2.1.27"
  },
  "devDependencies": {
    "eslint": "7.5.0",
    "eslint-plugin-zacanger": "2.0.5",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "engines": {
    "node": ">=6.0.0"
